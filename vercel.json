{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "WHATSAPP_SERVER_URL": "@whatsapp_server_url",
    "WHATSAPP_INSTANCE_NAME": "@whatsapp_instance_name",
    "WHATSAPP_API_KEY": "@whatsapp_api_key",
    "GOOGLE_GEMINI_API_KEY": "@google_gemini_api_key"
  }
}

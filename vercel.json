{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api.py"
    }
  ],
  "env": {
    "GOOGLE_GEMINI_API_KEY": "@google-gemini-api-key",
    "WHATSAPP_SERVER_URL": "@whatsapp-server-url",
    "WHATSAPP_INSTANCE_NAME": "@whatsapp-instance-name",
    "WHATSAPP_API_KEY": "@whatsapp-api-key"
  }
}